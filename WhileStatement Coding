package selfProgramming;

public class WhileStatement {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int i=1;
		while (i<5){
		  System.out.print(i + " ");
		  i++;   
		} System.out.println();
		
		int sum=0, number =1;
		  while (number <= 10)
		  {
			sum+=number;
			number = number + 5;
			System.out.println("SUM :" + sum);
		  }
		  System.out.println("Good Bye");

	}

}
